[machine]
	little-endian 8
[addressings]
	direto = 0 o
[instructions]
	08 NOP -    -   - 0000.0000
	16 STA a direto - 0001.0000a
	16 LDA a direto - 0010.0000a
	16 ADD a direto - 0011.0000a
	16 OR  a direto - 0100.0000a
	16 AND a direto - 0101.0000a
	08 NOT -    -   - 0110.0000
	16 JMP a direto - 1000.0000a
	16 JN  a direto - 1001.0000a
	16 JZ  a direto - 1010.0000a
	08 HLT -    -   - 1111.0000