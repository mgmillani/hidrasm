[machine]
	little-endian 8 queops
[addressings]
	direto = 0 o
	indireto = 1 a,i
	imediato = 10b #a
	relativo - 11b a+
[instructions]
	08 NOP - - - 0000.0000
	16 STR a * - 0001.0000a
	16 LDR a * - 0010.0000a
	16 ADD a * - 0011.0000a
	16 OR  a * - 0100.0000a
	16 AND a * - 0101.0000a
	08 NOT - - - 0110.0000
	16 JMP a * - 1000.0000a
	16 JN  a * - 1001.0000a
	16 JZ  a * - 1010.0000a
	16 JC  a * - 1011.0000a
	08 HLT - - - 1111.0000
